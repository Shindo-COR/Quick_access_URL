{
  "manifest_version": 3,
  "name": "URL Command Center",
  "version": "3.0",
  "permissions": ["storage", "tabs", "sidePanel"],
  "side_panel": {
    "default_path": "files/index.html"
  },
  "action": {
    "default_title": "URL Command Center"
  },
  "icons": {
    "128": "img/icon.png"
  },
  "background": {
  "service_worker": "controller/background.js"
  },
  "web_accessible_resources": [
		{
			"resources": ["img/*"],
			"matches": ["<all_urls>"]
		}
  ]
}
